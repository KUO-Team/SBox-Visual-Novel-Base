@using Sandbox.UI;
@using System;

@namespace VNBase.UI

@attribute [StyleSheet]
@inherits SubPanel

<root>
    @foreach (var character in Player.Characters)
    {
        <CharacterPortrait Character="@character" />
    }
</root>

@code {
    protected override int BuildHash()
    {
        return HashCode.Combine(Player.DialogueText, Player.Characters.Count);
    }
}