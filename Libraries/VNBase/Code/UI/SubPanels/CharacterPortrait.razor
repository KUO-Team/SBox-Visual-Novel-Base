@using Sandbox.UI;
@using System;
@using VNBase.Scripts;

@namespace VNBase.UI
@inherits SubPanel

<root class="portrait">
    <img src="@($"{Character.ActivePortraitPath}")" />
</root>

@code {
    public Character Character { get; set; }

    protected override int BuildHash()
    {
        return HashCode.Combine(Character, Character.ActivePortrait);
    }
}
