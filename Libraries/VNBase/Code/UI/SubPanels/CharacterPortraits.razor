@using Sandbox.UI;
@using System;

@namespace VNBase.UI
@inherits SubPanel

<root>
    @foreach (var character in Player.State.Characters)
    {
        <CharacterPortrait Character="@character" />
    }
</root>

@code {
    protected override int BuildHash()
    {
        return HashCode.Combine(Player.State.DialogueText, Player.State.Characters.Count);
    }
}